{
    "manifest_version": 2,
    "name": "Block addon",
    "description": "Addon to block websites.",
	"version": "1.0",
    "icons":
	{
		"32": "icon/AddonBlockIcon32x32.png",
		"64": "icon/AddonBlockIcon64x64.png"
	},

    "permissions":
	[
		"declarativeNetRequest",
		"nativeMessaging",
		"scripting",
		"tabs"
	],
	"background":
	{
		"scripts": ["./addon.block.js"],
		"persistent": false
	}
}